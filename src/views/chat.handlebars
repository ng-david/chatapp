<div id="loginModal" class="login-modal">
  <h1>Enter your name.</h1>
  <p id="err" class="err">Please enter a valid name.</p>
  <form id="nameInput" action="">
    <label for="name">Name:</label>
    <input autofocus id="name" autocomplete="off">
    <button>Start</button>
  </form>
</div>
<div id="overlay" class="overlay"></div>

<ul id="messages"></ul>
<form id="chatInput" action="">
  <input autofocus id="m" autocomplete="off">
  <button>Send</button>
</form>

<div>
  <ul id="activeUsers" class="active-users">
    
  </ul>
</div>
