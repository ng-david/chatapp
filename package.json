{
  "name": "ChenChat",
  "version": "0.8",
  "description": "A Socket.io powered webapp",
  "dependencies": {
    "express": "^4.15.2",
    "google-translate-api": "^2.3.0",
    "handlebars": "^4.0.10",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "start": "nodemon -e js,html,handlebars src/ --exec babel-node",
    "build": "rimraf build/ && babel src/ --out-dir build/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "serve": "node build/index.js"
  }
}
